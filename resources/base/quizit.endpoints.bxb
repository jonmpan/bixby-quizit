endpoints {
  action-endpoints {
    action-endpoint (Help) {
      accepted-inputs ($vivContext)
      local-endpoint (Help.js)
    }
    action-endpoint (GetQuiz) {
      accepted-inputs (category, difficulty, $vivContext)
      local-endpoint (GetQuiz.js)
    }
    action-endpoint (GetCategory) {
      accepted-inputs (categoryInput, $vivContext)
      local-endpoint (GetCategory.js)
    }
    action-endpoint (GetDifficulty) {
      accepted-inputs (difficultyInput, $vivContext)
      local-endpoint (GetDifficulty.js)
    }
    action-endpoint (GetRandomQuiz) {
      accepted-inputs ($vivContext)
      local-endpoint (GetRandomQuiz.js)
    }
    action-endpoint (NextQuestion) {
      accepted-inputs (quiz, $vivContext)
      local-endpoint (NextQuestion.js)
    }
    action-endpoint (CheckAnswer) {
      accepted-inputs (quiz, userAnswerString, $vivContext)
      local-endpoint (CheckAnswer.js)
    }
    action-endpoint (RepeatQuestion) {
      accepted-inputs (quiz)
      local-endpoint (RepeatQuestion.js)
    }
    action-endpoint (SkipTutorialAction) {
      accepted-inputs (skip, $vivContext)
      local-endpoint (SkipTutorialAction.js)
    }
    action-endpoint (RefreshQuestion) {
      accepted-inputs(quiz)
      local-endpoint (RefreshQuestion.js)
    }
    action-endpoint (ContinueQuiz) {
      accepted-inputs(quiz, $vivContext)
      local-endpoint (ContinueQuiz.js)
    }
    action-endpoint (ExtractAnswerFromModel) {
      accepted-inputs (answer)
      local-endpoint (ExtractAnswerFromModel.js)
    }
    // action-endpoint (BuildAudioInfo) {
    //   local-endpoint (BuildAudioInfo.js)
    // }
  }
}
