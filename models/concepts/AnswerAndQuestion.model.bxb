structure (AnswerAndQuestion) {
  description (Passes the question and the answer that the user chose)
  property (fullQuestion) {
    type (FullQuestion)
    min (Required) max (One)
  }
  property (userAnswer) {
    type (UserAnswer)
    min (Required) max (Many)
  }
  property (bixbyResponse) {
    type (BixbyResponse)
    min (Required) max (One)
  }  
 }
