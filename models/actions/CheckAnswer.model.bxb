action (CheckAnswer) {
  type(Search)
  description (__DESCRIPTION__)
  collect {
    input (quiz) {
      type (Quiz)
      min (Required) max (One)
    }
    input (userAnswer) {
      type (UserAnswer)
      min (Optional) max (Many)
    }
    input (userAnswerString) {
      type (UserAnswerString)
      min (Optional) max (One)
    }
  }
  output (Quiz)
}
